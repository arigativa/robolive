{
  "name": "robolive",
  "private": true,
  "scripts": {
    "start": "npm run build && npx serve -s ./build -p 3000",
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "typecheck": "tsc --project ./tsconfig.json",
    "lint": "npm run typecheck & eslint . --config=.eslintrc.prod.json --ext=js,ts,tsx",
    "prettify": "prettier . --write",
    "analyze": "source-map-explorer 'build/static/js/**/*.js' --gzip",
    "storybook": "start-storybook -p 9009 -s public",
    "storybook:build": "build-storybook -s public",
    "cy:open": "cypress open",
    "cy:run": "cypress run --browser chrome"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run typecheck",
      "pre-push": "lint-staged && npm run test -- --passWithNoTests --watchAll=false"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ],
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --config=.eslintrc.prod.json --fix"
    ]
  },
  "dependencies": {
    "@chakra-ui/react": "^1.0.2",
    "@emotion/react": "^11.1.1",
    "@emotion/styled": "^11.0.0",
    "@improbable-eng/grpc-web": "^0.13.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "browser-headers": "^0.4.1",
    "framer-motion": "^2.9.5",
    "frctl": "^2.0.0",
    "google-protobuf": "^3.14.0",
    "jssip": "^3.6.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8"
  },
  "devDependencies": {
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@storybook/addon-actions": "^6.1.10",
    "@storybook/addon-knobs": "^6.1.10",
    "@storybook/addon-links": "^6.1.10",
    "@storybook/addons": "^6.1.10",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "^6.1.10",
    "@types/jest": "^26.0.16",
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "cypress": "^5.6.0",
    "cypress-browser-permissions": "^1.0.5",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "husky": "^4.3.4",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "react-scripts": "^4.0.1",
    "serve": "^11.3.2",
    "source-map-explorer": "^2.5.1",
    "typescript": "^4.1.2"
  }
}
